{
  "name": "@gymu/api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "prisma": {
    "seed": "node --require ts-node/register prisma/seed.ts"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/src/index.js",
    "prod": "yarn build && yarn start",
    "dev": "prisma generate && nodemon src/index.ts --legacy-watch",
    "dev:db:push": "./setup.sh && yarn db:push && prisma db seed && yarn dev",
    "lint": "eslint . --ext .ts,.tsx",
    "prettier": "prettier --write .",
    "db:sync": "prisma db pull",
    "db:push": "prisma db push",
    "prisma:generate": "yarn prisma:map && prisma generate",
    "prisma:map": "npx ts-node prisma.mapper.ts && prettier --write prisma/schema.prisma"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.12.2",
    "@grpc/proto-loader": "^0.7.13",
    "@prisma/client": "^5.15.0",
    "bcrypt": "^5.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "http-proxy-middleware": "^3.0.3",
    "jsonwebtoken": "^9.0.2",
    "loadash": "^1.0.0",
    "pg": "^8.11.5",
    "pluralize": "^8.0.0",
    "prisma": "^5.15.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-dist": "^5.18.2",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.13",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.11.6",
    "@types/pluralize": "^0.0.33",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "@typescript-eslint/parser": "^7.12.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.0",
    "prettier": "^3.3.3",
    "prettier-plugin-prisma": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
